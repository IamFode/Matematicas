    "def induc1(n):\n",
    "    control = 0\n",
    "    for n in range(1,n+1):\n",
    "        control += n**2\n",
    "    print(control, \"=\", int((n*(n+1)*(2*n+1))/6))\n",
    "\n",
    "#ejemplos\n",
    "induc1(26)"
